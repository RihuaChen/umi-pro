{
  "name": "insures",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "start": "cross-env umi dev",
    "start:noMock": "cross-env MOCK=none umi dev",
    "dev": "yarn start:dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:dev-mock": "cross-env REACT_APP_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier -c --write \"**/*\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && yarn lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:fixJs": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less"
  },
  "dependencies": {
    "@formily/antd": "^1.3.13",
    "@formily/antd-components": "^1.3.13",
    "@umijs/preset-react": "1.x",
    "ahooks": "^2.10.0",
    "antd": "^4.14.1",
    "styled-components": "^5.2.1",
    "umi": "^3.4.5"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/react": "^17.x",
    "@types/react-dom": "^17.x",
    "@umijs/fabric": "^2.5.7",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/test": "^3.4.5",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.3",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.4",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "yarn lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,scss,md,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.scss",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
